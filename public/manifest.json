{
    "manifest_version": 3,
    "name": "Screeni",
    "version": "5.0.4",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon128.png",
      "default_height": "550",
      "default_width": "450"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "tabs",
      "storage",
      "notifications"
    ],

    "background": {
      "service_worker": "background.js"      
    },
  
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
  }

}
  